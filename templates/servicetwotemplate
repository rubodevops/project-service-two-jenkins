[Unit]
Description=Manage Java service
After=syslog.target network.target
[Service]
WorkingDirectory=/home/ubuntu/
ExecStart=/bin/java -Xmx256m -Xss32m -Djava.security.egd=file:/dev/urandom -jar service-two.jar
User=ubuntu
Type=simple
Restart=on-failure
RestartSec=10

[Install]
WantedBy=multi-user.target

